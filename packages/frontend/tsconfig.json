{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false
  },
  "include": [
    "src"
, "../epistogram_backend/src/shared/dtos/company/CompanyAssociatedCourseDTO.ts"  ],
  "exclude": [
    "node_modules",
  ]
}