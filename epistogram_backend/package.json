{
  "name": "epistogram_backend",
  "version": "0.3.0",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "main": "./server.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --ext .ts --fix",
    "build": "tsc",
    "mon": "npx nodemon src/server.ts",
    "tsdev": "tsnd --respawn server.ts",
    "node": "node --async-stack-traces server.ts",
    "nodenew": "nodemon -x node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/server.ts",
    "lr": "npx ts-node --esm --experimentalSpecifierResolution node ./src/server.ts --lightRecreate",
    "test": "jest -i ",
    "integtest": "jest -i StartCourse.test.ts",
    "testesm": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "confighooks": "git config core.hooksPath git_hooks"
  },
  "dependencies": {
    "@google-cloud/storage": "5.19.1",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.12",
    "@types/cron": "1.7.3",
    "@types/email-templates": "8.0.4",
    "@types/express-fileupload": "1.2.2",
    "@types/jsonwebtoken": "8.5.8",
    "@types/nodemailer": "6.4.4",
    "@types/pg": "8.6.5",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.0",
    "cron": "1.8.2",
    "dayjs": "1.11.0",
    "dotenv": "16.0.0",
    "express": "~4.17.3",
    "express-fileupload": "1.3.1",
    "get-video-duration": "^4.1.0",
    "moment": "2.29.3",
    "node-esm-loader": "^0.0.1",
    "password-generator": "2.3.2",
    "pg": "8.7.3",
    "react-scripts": "5.0.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.3.5",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "17.0.23",
    "copy-webpack-plugin": "10.2.4",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.3",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.7.3",
    "nodemon": "2.0.15",
    "ts-jest": "^28.0.7",
